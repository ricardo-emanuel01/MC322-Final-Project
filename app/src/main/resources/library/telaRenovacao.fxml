<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="library.TelaRenovacaoController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
            <Label layoutX="216.0" layoutY="73.0" text="Tela de Renovação">
               <font>
                  <Font size="25.0" />
               </font>
            </Label>
            <TextField fx:id="campoObra" layoutX="217.0" layoutY="148.0" />
            <Label layoutX="217.0" layoutY="117.0" text="Qual obra você tem em mãos?" />
            <Label layoutX="220.0" layoutY="200.0" text="Qual a data de empréstimo?" />
            <TextField fx:id="campoData" layoutX="220.0" layoutY="231.0" />
            <Button fx:id="botaoVoltar" layoutX="273.0" layoutY="320.0" onAction="#voltarParaUsuario" text="Voltar" />
            <Button fx:id="botaoRenovar" layoutX="273.0" layoutY="291.0" mnemonicParsing="false" onAction="#fazerRenovacao" text="Renovar" />
            <Label fx:id="labelMensagem" layoutX="220.0" layoutY="343.0" text="--" />
            <Label fx:id="labelNomeBiblioteca3" layoutX="206.0" layoutY="14.0" text="labelNomeBiblioteca3">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
      </children>
    </AnchorPane>
  </children>
</VBox>
