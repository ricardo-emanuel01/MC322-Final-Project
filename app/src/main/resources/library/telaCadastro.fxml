<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="library.TelaCadastroController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <Label fx:id="labelNomeBiblioteca1" layoutX="228.0" layoutY="14.0" text="labelNomeBiblioteca1">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
            <Label layoutX="228.0" layoutY="76.0" text="Cadastro">
               <font>
                  <Font size="17.0" />
               </font>
            </Label>
            <Label layoutX="228.0" layoutY="127.0" text="Primeio nome" />
            <Label layoutX="228.0" layoutY="191.0" text="Senha" />
            <Label layoutX="228.0" layoutY="253.0" text="Email" />
            <TextField fx:id="userCadastro" layoutX="228.0" layoutY="145.0" />
            <TextField fx:id="senhaCadastro" layoutX="229.0" layoutY="209.0" />
            <TextField fx:id="emailCadastro" layoutX="230.0" layoutY="271.0" />
            <CheckBox fx:id="usuarioCheck" layoutX="230.0" layoutY="308.0" mnemonicParsing="false" text="Usuário" />
            <CheckBox fx:id="adminCheck" layoutX="320.0" layoutY="308.0" mnemonicParsing="false" text="Funcionário" />
            <Button fx:id="botaoCadastrar" layoutX="250.0" layoutY="343.0" mnemonicParsing="false" onAction="#fazerCadastro" prefHeight="26.0" prefWidth="109.0" text="Cadastrar" />
            <Button fx:id="botaoSair" layoutX="590.0" layoutY="44.0" mnemonicParsing="false" onAction="#sair" text="Sair" />
         </children>
    </AnchorPane>
  </children>
</VBox>
