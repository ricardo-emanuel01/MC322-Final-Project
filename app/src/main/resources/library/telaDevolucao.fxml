            <Label layoutX="203.0" layoutY="69.0" text="Tela de Devolução">
               <font>
                  <Font size="25.0" />
               </font>
            </Label>
            <Label layoutX="219.0" layoutY="115.0" text="Qual obra você tem em mãos?" />
            <TextField fx:id="campoObra" layoutX="226.0" layoutY="147.0" />
            <Button fx:id="botaoVoltar" layoutX="281.0" layoutY="320.0" onAction="#voltarParaUsuario" text="Voltar" />
            <Button fx:id="botaoDevolver" layoutX="281.0" layoutY="279.0" mnemonicParsing="false" onAction="#fazerDevolucao" text="Devolver" />
            <Label fx:id="labelMensagem" layoutX="227.0" layoutY="336.0" text="--" />
            <Label fx:id="labelNomeBiblioteca3" layoutX="208.0" layoutY="14.0" text="labelNomeBiblioteca3">
               <font>
