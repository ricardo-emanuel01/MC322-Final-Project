<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="JavaFX.telaRenovacaoController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
            <Label layoutX="217.0" layoutY="39.0" text="Tela de Renovação">
               <font>
                  <Font size="25.0" />
               </font>
            </Label>
            <TextField fx:id="campoObra" layoutX="217.0" layoutY="148.0" />
            <Label layoutX="217.0" layoutY="117.0" text="Qual obra você tem em mãos?" />
            <Label layoutX="220.0" layoutY="200.0" text="Qual a data de empréstimo?" />
            <TextField fx:id="campoData" layoutX="220.0" layoutY="231.0" />
            <Button fx:id="botaoVoltar" layoutX="273.0" layoutY="320.0" text="Voltar" onAction="#voltarParaUsuario" />
            <Button fx:id="botaoRenovar" layoutX="273.0" layoutY="291.0" mnemonicParsing="false" onAction="#fazerRenovacao" text="Renovar" />
            <Label fx:id="labelMensagem" layoutX="220.0" layoutY="343.0" text="--" />
      </children>
    </AnchorPane>
  </children>
</VBox>
